        DEPLIBS = $(DEPXTESTLIB) $(DEPEXTENSIONLIB) $(DEPXLIB)
LOCAL_LIBRARIES = $(XTESTLIB) $(EXTENSIONLIB) $(XLIB)

           SRCS = xmany.c format.c displayAtom.c
           OBJS = xmany.o format.o displayAtom.o
  PROTO_DEFINES =

             CC = gcc
    CDEBUGFLAGS = -O9 -g
# CFLAGS += -funroll-loops -fomit-frame-pointer -finline-functions -mfpu
# CFLAGS += -fshort-enums -fcaller-saves
CFLAGS += -W -Wall -Wtraditional -Wshadow -Wpointer-arith -Wcast-qual -Winline
CFLAGS += -Waggregate-return


 PROGRAM = xmany

all:: xmany

xmany: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: xmany
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  xmany $(DESTDIR)$(BINDIR)/xmany

all:: xmany.0

xmany.0:: xmany.man
	@if [ -f xmany.man ]; \
	then \
	cat xmany.man | $(NEQN) | $(TBL) | $(NROFF) $(MANMACROS) | $(COL) >/tmp/$$$$.man; \
	cp /tmp/$$$$.man xmany.0; \
	$(RM) /tmp/$$$$.man; \
	fi

install.man::
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	-@if [ -f xmany.0 ]; \
	then \
	$(INSTALL) -c $(INSTMANFLAGS) xmany.0 $(DESTDIR)$(MANDIR)/xmany.0; \
	fi

clean::
	$(RM) xmany.0

depend:: .depend

.depend::
	$(RM) $@
	$(DEPEND) -f- $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS) > $@

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) xmany

# ----------------------------------------------------------------------
# common rules for all Makefiles - do not edit

.c.i:
	$(RM) $@
	 $(CC) -E $(CFLAGS) $(_NOOP_) $*.c > $@

emptyrule::

clean::
	$(RM) *.CKP *.ln *.BAK *.bak *.o core errs ,* *~ *.a .emacs_* tags TAGS make.log MakeOut *.core "#"*

Makefile::
	-@if [ -f Makefile ]; then set -x; \
	$(RM) Makefile.bak; $(MV) Makefile Makefile.bak; \
	else exit 0; fi
	$(IMAKE_CMD) -DTOPDIR=$(TOP) -DCURDIR=$(CURRENT_DIR)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

man_keywords::

# ----------------------------------------------------------------------
# empty rules for directories that do not have SUBDIRS - do not edit

