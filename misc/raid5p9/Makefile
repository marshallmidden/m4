PROG_r	= RAID-READ
PROG_w	= RAID-WRITE
NOMAN	=
CFLAGS += -W
CFLAGS += -Wall
CFLAGS += -Wshadow
CFLAGS += -Wpointer-arith
CFLAGS += -Wcast-qual
CFLAGS += -Winline
CFLAGS += -O4
CFLAGS += -g

#-----------------------------------------------------------------------------
LDFLAGS += -static
#-----------------------------------------------------------------------------
all:	${PROG_r} ${PROG_w}
#-----------------------------------------------------------------------------
${PROG_r}: ${PROG_r}.o
	${CC} ${CFLAGS} -o ${PROG_r} ${PROG_r}.o
#-----------------------------------------------------------------------------
${PROG_w}: ${PROG_w}.o
	${CC} ${CFLAGS} -o ${PROG_w} ${PROG_w}.o
#-----------------------------------------------------------------------------
clean:
	rm -f *.o ${PROG_r} ${PROG_w}
#-----------------------------------------------------------------------------
