#!/bin/bash -ex
FILE='FEIF'
#-----------------------------------------------------------------------------
run ()
{
    W="$1"
    P="$2"
    mkdir -p ${W}
    rm -f ${W}/${FILE}
    echo "$W -- ${P}"

    scp -p ~/fixefivars.py "root@${W}:fixefivars.py"

    ./fefi root ${W} ${P} > ${W}/${FILE} || true
}
