#!/bin/bash -ex
#-----------------------------------------------------------------------------
run ()
{
    W="$1"
    P="$2"
    mkdir -p ${W}
    echo "${P}"
    ./te root ${W} ${P} > ${W}/UUIDsAndEFI || true
}
#-----------------------------------------------------------------------------
# 172.22.11.x
run 172.22.11.13 "#2"
