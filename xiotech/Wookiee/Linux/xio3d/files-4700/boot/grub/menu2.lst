# Grub configuration for 3D 4700 X6DH8-XG2-based controllers
# Used for alternate boot when first drive may be ill.

default 0
timeout 4
serial --unit=0 --speed=115200
terminal serial console --timeout=2
hiddenmenu

###Don't change this comment - YaST2 identifier: Original name: linux###
title 3D (hd1)
    kernel (hd1,2)/boot/vmlinuz-3d root=/dev/md1 console=tty0 lm93.disable_block=1 maxcpus=2 e752x_edac.force_function_unhide=1 ipmi_watchdog.timeout=60 hugepages=230 xio3dshm=300m,400m,160m

###Don't change this comment - YaST2 identifier: Original name: linux###
title 3D Previous (hd1)
    kernel (hd1,2)/boot/vmlinuz-3d.prev root=/dev/md1 console=tty0 lm93.disable_block=1 maxcpus=2 e752x_edac.force_function_unhide=1 ipmi_watchdog.timeout=60 hugepages=230 xio3dshm=300m,400m,160m

###Don't change this comment - YaST2 identifier: Original name: Memory Test###
title Memory Test
    kernel --type=netbsd (hd1,2)/boot/memtest.bin
