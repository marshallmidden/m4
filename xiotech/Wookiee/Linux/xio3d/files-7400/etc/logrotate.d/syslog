# Syslog log rotation rules, modified for 3D.
#
# Note: all taken care of with logtrim. 2006-08-21
#
# /var/log/warn /var/log/messages /var/log/localmessages {
#     compress
#    maxage 365
#    rotate 10
#    missingok
#    notifempty
#    size +1024k
#    create 640 root root
#    sharedscripts
#    postrotate
#        /etc/init.d/syslog reload
#    endscript
#}
