#!/bin/bash -ex
echo "Starting $0"
#-----------------------------------------------------------------------------
. 00-package-definitions
. 00.COMPILE-WITH-NEW-GCC
#-----------------------------------------------------------------------------
cd clone/${MAKEDUMPFILE_DIR}
#-----------------------------------------------------------------------------
git checkout ${MAKEDUMPFILE_COMMIT}
#-----------------------------------------------------------------------------
${MAKE} clean || true
#-----------------------------------------------------------------------------
${MAKE} LINKTYPE=dynamic
#-----------------------------------------------------------------------------
${MAKE} install
#-----------------------------------------------------------------------------
# ${MAKE} clean || true
#-----------------------------------------------------------------------------
echo "Finishing $0"
exit 0
#-----------------------------------------------------------------------------
