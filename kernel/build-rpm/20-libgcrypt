#!/bin/bash -ex
#-----------------------------------------------------------------------------
echo "Starting $0"
#-----------------------------------------------------------------------------
. 00-package-definitions
. 00.COMPILE-WITH-NEW-GCC
#-----------------------------------------------------------------------------
# Do libgpg-error first.
#-----------------------------------------------------------------------------
cd ${LIBGPG_ERROR_VERSION}
#-----------------------------------------------------------------------------
./configure --prefix=${BINARIES} CFLAGS=-gdwarf-4 1>&2
#-----------------------------------------------------------------------------
${MAKE} ${J}
#-----------------------------------------------------------------------------
${MAKE} install
#-----------------------------------------------------------------------------
#-- ${MAKE} clean || true
#-- ${MAKE} distclean || true
#-----------------------------------------------------------------------------
#-----------------------------------------------------------------------------
cd ..
#-----------------------------------------------------------------------------
#-----------------------------------------------------------------------------
cd ${LIBGCRYPT_VERSION}
#-----------------------------------------------------------------------------
./configure --prefix=${BINARIES} CFLAGS=-gdwarf-4 1>&2
#-----------------------------------------------------------------------------
${MAKE} ${J}
#-----------------------------------------------------------------------------
${MAKE} install
#-----------------------------------------------------------------------------
#-- ${MAKE} clean || true
#-- ${MAKE} distclean || true
#-----------------------------------------------------------------------------
ldconfig                        # Put it back to original.
#-----------------------------------------------------------------------------
echo "Finishing $0"
exit 0
#-----------------------------------------------------------------------------
