tempo   120,4

decode  m1,3c
decode  m1,m2,3c4

decode  m1,m2,m3,3c4dls
decode  m1,m2,m3,3c4dsl

decode  m1,m2,m3,3c4das
decode  m1,m2,m3,3c4dsa

decode  m1,m2,m3,3c4sza
decode  m1,m2,m3,3c4saz

marker m1 * 1000
