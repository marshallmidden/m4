* vim: tabstop=8 expandtab shiftwidth=4 softtabstop=4
*    60 = 3c                    61 = 3c+ 3d-            62 = 3d
*    63 = 3d+ 3e-               64 = 3e                 65 = 3f
*    66 = 3f+ 3f-               67 = 3g                 68 = 3g+ 3g-
*    69 = 3a                    70 = 3a+ 3b-            71 = 3b
*    72 = 4c                    73 = 4c+ 4d-            74 = 4d
*    75 = 4d+ 4e-               76 = 4e                 77 = 4f
*    78 = 4f+ 4g-
*    ------------------------------------------------------------------------------
*    # 6 flats  is G- major or E- minor. b-,e-,a-,d-,g-,c-
*    ------------------------------------------------------------------------------
volumes f,f
voice   2
meter   3/16
bars    1
macro viola,VOICE
  instrument VOICE Viola
  intensity  VOICE 100
  pan        VOICE 70
  reverb     VOICE 60
endm
putd    =violaA=1=
putd    =violaB=2=
=clef   violaA..violaB alto                     $$ viola
=key     violaA..violaB                    g-   $$ viola - six flats
measure 1
=v violaA: 4g8, 4c-16
=v violaB: 3b8, 4c-16
