* All staves.
=%%staves \
   [(brass1A brass1B brass1C brass1D brass1E brass1F) \
    (brass2A brass2B brass2C brass2D brass2E brass2F)] \
   [(piccoloA) (flute1A) (flute2A)] \
   [(oboeA oboeB) (clarinetA clarinetB) (english_hornA) (bassoonA bassoonB)] \
   [(french_horn1A french_horn1B) (french_horn2A french_horn2B)] \
   [(cornet1A cornet1B) (trumpet1A trumpet1B)] \
   [(trombone1A trombone1B) (tuba1A tuba1B)] \
   timpaniA [tambourineA] [triangleA] [snaredrumA] \
   [cymbalA bassdrumA] \
   [bellsA] [canonA] \
   [(violin1A violin1B violin1C violin1D) (violin2A violin2B violin2C violin2D) \
    (violaA violaB violaC violaD) \
    (cello1A cello1B cello1C cello1D) (cello2A cello2B cello2C cello2D) (contrabassA)]
endif
* ----------------------------------------------------------------------------
=volumes brass1A..contrabassA mf
* ----------------------------------------------------------------------------
page    3           $$ yaps and abcm2ps do better with a blank thing right away.
measure 0
meter   3/4
*--? bars    7+1    $$ first one is 1 more than there are.
=key    brass1A..brass1F    e-                  $$ 3 flats
=key    brass2A..brass2F   e-                   $$ 3 flats
=key    piccoloA    e-                          $$ 3 flats
=key    flute1A    e-                           $$ 3 flats
=key    flute2A    e-                           $$ 3 flats
=key    oboeA..oboeB   e-                       $$ 3 flats
=key    clarinetA..clarinetB    f               $$ 1 flat
=key    english_hornA   b-                      $$ 2 flats
=key    bassoonA..bassoonB   e-                 $$ 3 flats
=key    french_horn1A..french_horn1B   c        $$ no flats
=key    french_horn2A..french_horn2B   c        $$ no flats
=key    cornet1A..cornet1B    f                 $$ 1 flat
=key    trumpet1A..trumpet1B    c               $$ no flats
=key    trombone1A..trombone1B   e-             $$ 3 flats
=key    tuba1A..tuba1B   e-                     $$ 3 flats
=key    timpaniA       c                        $$ no flats
=key    tambourineA c                           $$ tambourine
=key    triangleA       c                       $$ no flats
=key    snaredrumA       c                      $$ no flats
=key    cymbalA   c                             $$ no flats
=key    bassdrumA   c                           $$ no flats
=key    bellsA   c                              $$ no flats
=key    canonA   c                              $$ no flats
=key    violin2A..violin2D e-                   $$ 3 flats
=key    violaA..violaD e-                       $$ 3 flats
=key    cello1A..cello1D e-                     $$ 3 flats
=key    cello2A..cello2D e-                     $$ 3 flats
=key    contrabassA  e-                         $$ 3 flats
* ----------------------------------------------------------------------------
page    3
measure 1      $$ Page 3, 1st
bars    7+1         $$ first one is 1 more than there are.
=clef   cello1A..cello1D tenor                  $$ cello #1
=v brass1A: r4,r4,r4
=v brass1B: r4,r4,r4
=v brass1C: r4,r4,r4
