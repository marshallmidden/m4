#!/bin/bash -eupx
# ----------------------------------------------------------------------------
V="$*"
if [ "$V"x == "x" ]; then
    echo "Need one argument of voice number."
    exit 1
fi
# ----------------------------------------------------------------------------
egrep "^measure |^page |^v${V}: " v2-2.gcs | grep -v "^v${V}: r4d" | grep -v "^v${V}: r8, *r8, *r8$"
# ----------------------------------------------------------------------------
# ----------------------------------------------------------------------------
# ----------------------------------------------------------------------------
