*-- include piano,standard print stuff
#include "standard-print-stuff.include"

staff   right

staff   left

time    4,4

format  systems 1,6,11,16,21;26,31,36,41,46;

format  lyrics offset 40

mytitle True Faith,by New Order  (arranged by Tom Cortese)

*

*

*-- include piano,process invert
#include "process-invert.include"

calc    inverting ^= -1

*

measure 1

if      inverting

.       invtitl

.       right:  begin invert

.       left:   begin invert

.       print   left,right

.       right:  bass key f major

.       left:   treble key f major

else

.       right:  treble key d minor

.       left:   bass key d minor

endif

