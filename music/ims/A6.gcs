* vim: tabstop=8 expandtab shiftwidth=4 softtabstop=4

macro   hz(FREQ,LTH)
        vars    notenum
        calc    notenum = 12*([log(FREQ/13.75)]/[log(2)])-3
        calc    notenum = min(notenum,110)
        calc    notenum = max(notenum,1)
                (notenum)(LTH)
endmacro
*
staff   s1:1,2
staff   s2:3,4,5
measure 1
s1:     t(3c,3c+,2)4 3d4
s2:     4c4 [4d8 4e8] [4f8 4g8]

*+ measure 2
*+ s1,s2: [2c4,2d8] 2e8
*+ 
*+ measure 3
*+ s1:     hz(440,.25)
cstop*
