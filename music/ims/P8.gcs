macro   func(ARG)
debug func #1, a , x , y , i , ARG , TIMES
.       calc  ARG =(a*(3.77823-a))*ARG(1-ARG)
debug func #2, a , x , y , i , ARG , TIMES
endmacro
*-----------------------------------------------------------------------------
macro   iterate(ARG,TIMES)
.       vars    i
debug iterate #1, a , x , y , i , ARG , TIMES
.       func(ARG)
debug iterate #2, a , x , y , i , ARG , TIMES
endmacro
