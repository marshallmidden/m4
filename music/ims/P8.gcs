debug main #2, a , x , y , i , ARG , TIMES
calc    x = (1/32)
debug main #3, a , x , y , i , ARG , TIMES
calc    y = x
debug main #4, a , x , y , i , ARG , TIMES
iterate(y,10)
debug main #5, a , x , y , i , ARG , TIMES
