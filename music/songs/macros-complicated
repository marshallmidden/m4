* do a U1 to get past macro definitions.
*
calc    m5=5
macro   treble
calc    m2=2
calc    m3=3
calc    m4=4
endm
*
macro   bass
calc    m2=6
calc    m3=7
calc    m4=8
endm
*
macro   c33,A,B,C
 
v(m2):A
v(m3):B
v(m4):C
endm
*
macro   c32,A,B
decode  m22,A
 
v(m2):A
v(m3):B
v(m4):r(m22)
endm
*
macro   c31,A
decode  m22,A
 
v(m2):A
v(m3):r(m22)
v(m4):r(m22)
endm
*
macro   c44,A,B,C,D
 
v(m2):A
v(m3):B
v(m4):C
v(m5):D
endm
*
macro   c43,A,B,C
decode  m22,A
 
v(m2):A
v(m3):B
v(m4):C
v(m5):r(m22)
endm
*
macro   c42,A,B
decode  m22,A
 
v(m2):A
v(m3):B
v(m4):r(m22)
v(m5):r(m22)
endm
*
macro   c41,A
decode  m22,A
 
v(m2):A
v(m3):r(m22)
v(m4):r(m22)
v(m5):r(m22)
endm
*
macro   c22,A,B
 
v(m2):A
v(m3):B
endm
*
macro   c21,A
decode  m22,A
 
v(m2):A
v(m3):r(m22)
endm
