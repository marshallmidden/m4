measure 1
macro   alberti,ua,ub,la,lb,uc,lc
decode  m1,ua
decode  m2,ub
decode  m3,la
decode  m4,lb
decode  m5,uc
decode  m6,lc
(m1+12)16,(m1+24)16s,la16,(m3+12)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m2+11)16,(m2+23)16s,(m4-1)16,(m4+11)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m5+12)16,(m5+24)16s,lc16,(m6+12)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m2+11)16,(m2+23)16s,(m4-1)16,(m4+11)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m1+12)16,(m1+24)16s,la16,(m3+12)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m2+11)16,(m2+23)16s,(m4-1)16,(m4+11)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m5+12)16,(m5+24)16s,lc16,(m6+12)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m2+11)16,(m2+23)16s,(m4-1)16,(m4+11)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
endm
macro   alberto,ua,ub,la,lb,uc,lc
decode  m1,ua
decode  m2,ub
decode  m3,la
decode  m4,lb
decode  m5,uc
decode  m6,lc
(m1+12)16,(m1+24)16s,la16,(m3+12)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m2+10)16,(m2+22)16s,(m4-2)16,(m4+10)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m5+12)16,(m5+24)16s,lc16,(m6+12)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m2+10)16,(m2+22)16s,(m4-2)16,(m4+10)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m1+12)16,(m1+24)16s,la16,(m3+12)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m2+10)16,(m2+22)16s,(m4-2)16,(m4+10)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m5+12)16,(m5+24)16s,lc16,(m6+12)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
(m2+10)16,(m2+22)16s,(m4-2)16,(m4+10)16s
(m2+12)16,(m2+24)16s,lb16,(m4+12)16s
endm
