#!/usr/local/bin/bash -x
# vim: tabstop=8 expandtab shiftwidth=4 softtabstop=4
#-----------------------------------------------------------------------------
# First two arguments are: 
#   1) The original big file before swapping lines.
#   2) The file to create after swapping lines.
# Next ones are in groups of two:
#   3) pattern1, and pattern2 - the lines to swap.
#      '=clarinetA:'   '=clarinetB:'
#-----------------------------------------------------------------------------
./04.SWAP 113 113.new \
    '=violin1A:'        '=violin1B:'        \
    '=violin2A:'        '=violin2B:'        \
    '=trombone2A:'      '=trombone2B:'      \
    '=trumpet1A:'       '=trumpet1B:'       \
    '=trombone1A:'      '=trombone1B:'      \
    '=cello1A:'         '=cello1B:'         \
    '=cello2A:'         '=cello2B:'         \
    '=cornet1A:'        '=cornet1B:'        \

#-----------------------------------------------------------------------------
exit 0
*   '=oboeA:'           '=oboeB:'           \
*   '=bassoonA:'        '=bassoonB:'        \
*   '=clarinetA:'       '=clarinetB:'       \
*   '=french_horn1A:'   '=french_horn1B:'   \
*   '=french_horn2A:'   '=french_horn2B:'   \
*   '=violaA:'          '=violaB:'          \
#-----------------------------------------------------------------------------
exit 0
